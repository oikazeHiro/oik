<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oik.dao.mapper.ChatMsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.oik.dao.entity.ChatMsg">
        <id column="id" property="id"/>
        <result column="send_id" property="sendId"/>
        <result column="accept_id" property="acceptId"/>
        <result column="msg" property="msg"/>
        <result column="sign_flag" property="signFlag"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , send_id, accept_id, msg, sign_flag, create_time
    </sql>

</mapper>
