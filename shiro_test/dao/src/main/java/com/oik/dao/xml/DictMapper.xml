<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oik.dao.mapper.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.oik.dao.entity.Dict">
        <id column="DICT_ID" property="dictId"/>
        <result column="KEYY" property="keyy"/>
        <result column="VALUEE" property="valuee"/>
        <result column="FIELD_NAME" property="fieldName"/>
        <result column="TABLE_NAME" property="tableName"/>
        <result column="CREATE_TIME" property="createTime"/>
        <result column="CREATE_USERNAME" property="createUsername"/>
        <result column="CREATE_USER_ID" property="createUserId"/>
        <result column="MODIFY_TIME" property="modifyTime"/>
        <result column="MODIFY_USERNAME" property="modifyUsername"/>
        <result column="MODIFY_USER_ID" property="modifyUserId"/>
        <result column="STATUS" property="status"/>
        <result column="OTHER_KEYY" property="otherKeyy"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        DICT_ID
        , KEYY, VALUEE, FIELD_NAME, `TABLE_NAME`, CREATE_TIME, CREATE_USERNAME, CREATE_USER_ID, MODIFY_TIME, MODIFY_USERNAME, MODIFY_USER_ID, `STATUS`, OTHER_KEYY
    </sql>

</mapper>
