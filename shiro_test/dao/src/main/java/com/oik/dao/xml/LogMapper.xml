<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oik.dao.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.oik.dao.entity.Log">
        <id column="ID" property="id"/>
        <result column="USERNAME" property="username"/>
        <result column="OPERATION" property="operation"/>
        <result column="TIME" property="time"/>
        <result column="METHOD" property="method"/>
        <result column="PARAMS" property="params"/>
        <result column="IP" property="ip"/>
        <result column="CREATE_TIME" property="createTime"/>
        <result column="location" property="location"/>
        <result column="RESPONSE_CODE" property="responseCode"/>
        <result column="RESPONSE_TEXT" property="responseText"/>
        <result column="TYPE" property="type"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID
        , USERNAME, OPERATION, `TIME`, METHOD, PARAMS, IP, CREATE_TIME, location, RESPONSE_CODE, RESPONSE_TEXT, `TYPE`
    </sql>

</mapper>
